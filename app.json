{
  "expo": {
    "name": "FlyMaster",
    "slug": "flymaster-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "flymaster",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "router": {
      "root": "app"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flymaster.app"
    },
    "android": {
      "package": "com.flymaster.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", {"root": "app"}], 
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}